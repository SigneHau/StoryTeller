{
  "expo": {
    "name": "storyteller",
    "slug": "storyteller",
    "version": "1.0.0",
    "scheme": "storyteller",
    "orientation": "portrait",
    "icon": "./assets/mascot.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mascot.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mascot.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/mascot.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ]
  }
}
